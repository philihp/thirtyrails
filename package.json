{
  "name": "thirtyrails",
  "author": "Philihp Busby <philihp@gmail.com>",
  "license": "MIT",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "dev": "next dev",
    "build": "next build",
