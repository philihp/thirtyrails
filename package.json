{
  "name": "thirtyrails",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "next": "9.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@philihp/eslint-config": "0.0.5",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.8"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --fix"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "@philihp"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  }
}
